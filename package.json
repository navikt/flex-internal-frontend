{
    "name": "flex-internal-frontend",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "20"
    },
    "dependencies": {
        "@navikt/ds-css": "^6.16.3",
        "@navikt/ds-react": "^6.16.3",
        "@navikt/ds-tailwind": "^6.16.3",
        "@navikt/ds-tokens": "^6.1.0",
        "@navikt/next-api-proxy": "^4.1.3",
        "@navikt/next-logger": "^1.35.0",
        "@navikt/oasis": "^3.6.0",
        "@tanstack/react-query": "^5.59",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.12",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-json-view-lite": "^2.0.1",
        "tailwind-merge": "^2.6.0",
        "uuid": "^11.0.4"
    },
    "devDependencies": {
        "@navikt/eslint-config-teamsykmelding": "^7.1.0",
        "@types/node": "^20.14.10",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/uuid": "^10.0.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-config-next": "^14.2.15",
        "next": "^14.2.15",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.7.2"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev -p 8080",
        "local": "LOCAL_BACKEND=true MOCK_BACKEND=true next dev -p 8080",
        "start": "next start -p 8080",
        "build": "next build",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-non-null-assertion": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
