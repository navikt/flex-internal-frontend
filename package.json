{
    "name": "flex-internal-frontend",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "20"
    },
    "dependencies": {
        "@navikt/ds-css": "^5.17.1",
        "@navikt/ds-react": "^5.17.1",
        "@navikt/ds-tailwind": "^5.17.1",
        "@navikt/ds-tokens": "^5.6.1",
        "@navikt/next-api-proxy": "^3.4.0",
        "@navikt/next-logger": "^1.21.0",
        "@tanstack/react-query": "^4.36.1",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.10",
        "openid-client": "^5.6.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^2.2.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@navikt/eslint-config-teamsykmelding": "^5.1.0",
        "@types/node": "^20.10.4",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@types/uuid": "^9.0.7",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-config-next": "^14.1.0",
        "next": "^14.1.0",
        "prettier": "^3.1.1",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev -p 8080",
        "start": "next start -p 8080",
        "build": "next build",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-non-null-assertion": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
